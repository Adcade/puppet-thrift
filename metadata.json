{
  "name": "spantree-thrift",
  "version": "1.1.1",
  "author": "spantree",
  "summary": "Download, compile and install thrift",
  "license": "MIT",
  "source": "https://github.com/Spantree/puppet-thrift.git",
  "project_page": "https://github.com/spantree/puppet-thrift",
  "issues_url": "https://github.com/spantree/puppet-thrift/issues",
  "description": "This is a fork from https://github.com/Adcade/puppet-thrift.git",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "camptocamp-archive",
      "version_requirement": ">=0.9.0"
    },
    {
      "name": "maestrodev-ant",
      "version_requirement": ">=1.0.5"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.5.0 <5.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=3.7.0 <=5.5.0"
    }
  ]
}
